
/* swiper默认的宽是320px高是150px 
图片默认宽是320，高是240px */

.swiper-index{
    width: 750rpx;
    height: 340rpx;
}
.swiper-index .img{
    width: 100%;
}

/* 导航 */
.index_cate{
    display: flex;
    margin-top: 10px;
    margin-left: 10px;
}
.index_cate navigator {
    flex: 1;
  }
  .index_cate navigator image {
    width: 80%;
  }

/* 楼层 */

.floor-title{
    padding: 10rpx 0;  
}

.floor-content{
    overflow: hidden;
     
}
.floor-content navigator{
    width: 33.33%;
    float: left;
}
.floor-content navigator:nth-last-child(-n+4) {
    height: 27.72711207vw;
    border-left: 10rpx solid #fff;
  }

  .floor-content  navigator:nth-child(2),
  .floor-content  navigator:nth-child(3) {
    border-bottom: 10rpx solid #fff;
  }
  

.floor-content navigator image{
    width: 100%;
    height: 100%;
}